<body>
    @if (ObtenerRol() == "Administrador")
    {
        <section class="px-5 ">
            <div class="col-12 px-5 ">
                <h2 class="fw-bold">Menu</h2>
                <div class="row mb-4">
                    <div class="col mb-4">
                        <div class="card-group">
                            <div class="card">
                                <div class="card-body" style="background: #0b5ed70d" ;>
                                    <svg class="card-img" width="100" height="100"><use xlink:href="#Vender" /></svg>
                                    <h3 class="card-title fw-bold"> Vender </h3>
                                    <p class="card-text">Optimisar tu tiempo es importante, crea ventas de manera rapida.</p>
                                    <a href="@Url.Action("Index","Venta")" class="btn btn-primary">Ingresar</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col mb-4">
                        <div class="card-group">
                            <div class="card">
                                <div class="card-body" style="background: #0b5ed70d" ;>
                                    <svg class="card-img" width="100" height="100"><use xlink:href="#Inventario" /></svg>
                                    <h3 class="card-title fw-bold"> Inventario </h3>
                                    <p class="card-text">Controla cada uno de los productos que ingresan a tu libreria.</p>
                                    <a href="@Url.Action("Inventario","Home")" class="btn btn-primary">Ingresar</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
        <section class="px-5">
            <div class="col-12 px-5 ">
                <div class="row mb-4">


                    <div class="col mb-4">
                        <div class="card-group">
                            <div class="card">
                                <div class="card-body" style="background: #0b5ed70d" ;>
                                    <svg class="card-img" width="100" height="100"><use xlink:href="#Mantenimiento" /></svg>
                                    <h3 class="card-title fw-bold"> Mantenimiento </h3>
                                    <p class="card-text"> Establece y crea nuevas ,marca categorias ,productos etc.</p>
                                    <a href="@Url.Action("Mant","Home")" class="btn btn-primary">Ingresar</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>


    }
    else if (ObtenerRol() == "Vendedor")
    {
        <section class="px-5 ">
            <div class="col-12 px-5 ">
                <h2>Menu</h2>
                <div class="row mb-4">
                    <div class="col mb-4">
                        <div class="card-group">
                            <div class="card">
                                <div class="card-body" style="background: #0b5ed70d" ;>
                                    <svg class="card-img" width="100" height="100"><use xlink:href="#Vender" /></svg>
                                    <h3 class="card-title fw-bold"> Vender </h3>
                                    <p class="card-text">Optimisar tu tiempo es importante, crea ventas de manera rapida.</p>
                                    <a href="@Url.Action("Index","Venta")" class="btn btn-primary">Ingresar</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="col mb-4">
                        <div class="card-group">
                            <div class="card">
                                <div class="card-body" style="background: #0b5ed70d" ;>
                                    <svg class="card-img" width="100" height="100"><use xlink:href="#Inventario" /></svg>
                                    <h3 class="card-title fw-bold"> Inventario </h3>
                                    <p class="card-text">Controla cada uno de los productos que ingresan a tu libreria.</p>
                                    <a href="@Url.Action("Inventario","Home")" class="btn btn-primary">Ingresar</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </section>
    }
