
@{
    ViewBag.Title = "Mantenimiento";
    Layout = "~/Views/shared/_Layout.cshtml";
    string ObtenerRol()
    {
        return Session["RolSesion"] != null ? Session["RolSesion"].ToString() : "";
    }
}
@if (TempData["SwalAlert1"] != null)
{
    <script>
        Swal.fire({
            icon: "success",
            title: "@TempData["SwalAlert1"].ToString()",
            html: "<div>Bienvenido, @Session["Nombre"].ToString()!</div>",
            showConfirmButton: false,
            timer: 1500
        });
    </script>
}




